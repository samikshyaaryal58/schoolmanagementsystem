@model schoolmanagementsystem.Models.ViewModels.TeacherViewModels.UpdateTeacherViewModel;
<div class="wrapper">
    <form method="post">
        <div class="mb-3">
            <label for="recipient-name" class="col-form-label">Student Id</label>
            <input type="text" class="form-control" id="recipient-name" readonly placeholder="Product Name" asp-for="StudentId">
        </div>
        <div class="mb-3">
            <label for="recipient-name" class="col-form-label">Student Name</label>
            <input type="text" class="form-control" id="recipient-name" placeholder="Name" asp-for="Name">
        </div>
        <div class="mb-3">
            <label for="recipient-name" class="col-form-label"> Image url</label>
            <input type="text" class="form-control" id="recipient-name" placeholder="Imgaes" asp-for="Picture">
        </div>
        <div class="mb-3">
            <label for="recipient-name" class="col-form-label">Address</label>
            <input type="text" class="form-control" id="recipient-name" placeholder="Address" asp-for="Address">
        </div>
        <div class="mb-3">
            <label for="recipient-name" class="col-form-label">PhoneNumber</label>
            <input type="text" class="form-control" id="recipient-name" placeholder="PhoneNumber" asp-for="PhoneNumber">
        </div>

        <div>

            <button type="submit" class="btn btn-primary">Update Student</button>

        </div>
    </form>
</div>